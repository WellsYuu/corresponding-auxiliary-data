<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo
       http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
	<dubbo:application name="demo-consumer" />
	<!--<dubbo:registry address="multicast://224.5.6.7:1234?unicast=false"
		register="true" />-->
	<dubbo:registry protocol="zookeeper" address="192.168.0.147:2181" check="true"/>

	<!--<dubbo:registry protocol="redis" address="192.168.0.147:6379" check="true"/> -->
	<dubbo:reference id="demoService" timeout="2000"
		interface="com.tuling.teach.service.DemoService" check="false" />
<!--  loadbalance="consistenthash"  
 -->	
 <dubbo:reference id="userService" timeout="2000"
		interface="com.tuling.teach.service.UserService" 
		loadbalance="consistenthash"
		 check="false" >
	 <dubbo:parameter key="hash.arguments" value="0,1"/>
	 <dubbo:parameter key="hash.nodes" value="320"/>
	</dubbo:reference>
</beans>