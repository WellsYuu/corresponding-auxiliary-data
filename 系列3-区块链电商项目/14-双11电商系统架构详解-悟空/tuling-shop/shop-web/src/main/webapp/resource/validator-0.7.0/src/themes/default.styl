/* theme: default */
.n-default {
    .n-left, .n-right {
        margin-top: 5px;
    }
    .n-top {
        .msg-wrap {
            bottom: 100%;
        }
    }
    .n-bottom {
        .msg-wrap {
            top: 100%;
        }
    }
    .msg-wrap {
        position: absolute;
        z-index: 1;
       .n-icon {
            background-image: url(images/validator_default.png);
        } 
    }
    .n-tip {
        .n-icon {display:none;}
    }
}
