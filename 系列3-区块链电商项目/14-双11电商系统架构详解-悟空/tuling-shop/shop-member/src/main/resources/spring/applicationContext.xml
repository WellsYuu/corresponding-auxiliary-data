<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xmlns:aop="http://www.springframework.org/schema/aop" xmlns:tx="http://www.springframework.org/schema/tx"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context  
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">
    <context:component-scan base-package="com.jiagouedu.*" />
    <import resource="classpath*:spring/applicationContext-dao.xml"/>
    <import resource="classpath*:spring/applicationContext-base.xml"/>
    <import resource="classpath*:spring/applicationContext-cache.xml"/>
    <import resource="classpath*:dubbo/dubbo-members-provider.xml"/>


</beans>