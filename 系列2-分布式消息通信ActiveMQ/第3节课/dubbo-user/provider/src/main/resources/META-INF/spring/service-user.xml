<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd" default-autowire="byName">


    <!--服务发布的配置，需要暴露的服务接口-->
    <dubbo:service interface="com.gupao.vip.mic.dubbo.user.IUserCoreService"
                   ref="userLoginService" timeout="3000"/>

    <dubbo:service interface="com.gupao.vip.mic.dubbo.user.IUserQueryService"
                   ref="userQueryService" timeout="3000"/>


</beans>